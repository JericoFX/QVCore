{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "dynamicImport": true,
        "decorators": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "target": "es2016",
      "experimental": {
        "keepImportAssertions": true
      },
      "minify": {
        "compress": {
          "unused": true
        },
        "mangle": true
      }
    },
    "minify": false
}
